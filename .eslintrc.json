{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-useless-catch": "off",
        "no-restricted-syntax":"off",
        "guard-for-in": "off",
        "consistent-return":"off",
        "no-underscore-dangle":"off",
        "comma-dangle":"off",
        "no-plusplus":"off",
        "eqeqeq":"off",
        "no-undef": "off",
        "prefer-const":"off",
        "arrow-body-style":"off",
        "no-else-return":"off",
        "linebreak-style": [2, "windows"],
        "no-extraneous-dependencies":"off"
    }
}